{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "turbo run dev",
    "dev:control": "turbo run dev --filter=control",
    "dev:api": "turbo run dev --filter=api",
    "ios": "turbo run ios",
    "storybook": "turbo run storybook",
    "storybook:expo:ios": "turbo run storybook:expo:ios",
    "storybook:expo:android": "turbo run storybook:expo:android",
    "generate": "turbo run generate --filter=api && turbo run generate --filter=@repo/types",
    "build": "turbo run build",
    "build:control": "turbo run build --filter=control",
    "build:api": "turbo run build --filter=api",
    "split": "turbo run split",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "prepare": "node -e \"if (!process.env.CI) { const {execSync} = require('child_process'); try { execSync('husky', {stdio: 'inherit', env: process.env}); } catch(e) { process.exit(0); } }\"",
    "kill": "lsof -ti :3000,3001 | xargs kill -9",
    "docker:build": "docker compose -f docker-compose.dev.yml build",
    "docker:dev": "docker compose -f docker-compose.dev.yml up"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@repo/typescript-config": "*",
    "husky": "^9.1.7",
    "ts-jest": "^29.4.6",
    "turbo": "^2.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@nestjs/bullmq": "^11.0.4",
    "@svgr/webpack": "^8.1.0",
    "bullmq": "^5.66.4",
    "cross-env": "^10.1.0",
    "openai": "^5.23.1",
    "pino": "^9.9.0"
  }
}
